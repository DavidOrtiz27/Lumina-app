{
  "expo": {
    "name": "Lumina-app",
    "slug": "Lumina-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/LuminaIcon.png",
    "scheme": "luminaapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#FFFFFF",
        "foregroundImage": "./assets/images/LuminaIcon.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.davidortiz.Luminaapp"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/LuminaIcon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/LuminaIcon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "expo-media-library",
        {
          "photosPermission": "Permitir que $(PRODUCT_NAME) guarde el código QR en tu galería",
          "savePhotosPermission": "Permitir que $(PRODUCT_NAME) guarde el código QR en tu galería",
          "isAccessMediaLocationEnabled": true
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "582c1edb-aac3-4896-8d9c-c9b8ff4b80b2"
      }
    },
    "owner": "davidortiz"
  }
}
